{
  "version": 2,
  "functions": {
    "api/xumm.js": {
      "runtime": "nodejs18.x"
    }
  },
  "builds": [
    {
      "src": "api/xumm.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/xumm",
      "dest": "/api/xumm.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}